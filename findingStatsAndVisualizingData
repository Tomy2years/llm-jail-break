      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "from matplotlib import cm\n",
        "\n",
        "xlabels = (\"Direct injection\", \"Role-Playing\", \"Context Manipulation\", \"Obfuscation\")\n",
        "success_rates = {\n",
        "    'Gemini 2.5 Flash': (1, 3.25, 3.6, 0),\n",
        "    'GPT4o-mini': (2, 2.43, 2.3, 1.9),\n",
        "}\n",
        "\n",
        "x = np.arange(len(xlabels))  # the label locations\n",
        "width = 0.25  # the width of the bars\n",
        "multiplier = 0\n",
        "\n",
        "fig, ax = plt.subplots(layout='constrained')\n",
        "\n",
        "colors = ['#365c8d', 'grey']\n",
        "for data, color in zip(success_rates.items(), colors):\n",
        "    attribute, rates = data\n",
        "    offset = width * multiplier\n",
        "    rects = ax.bar(x + offset, rates, width, label=attribute, color=color)\n",
        "    ax.bar_label(rects, padding=3)\n",
        "    multiplier += 1\n",
        "\n",
        "# Add some text for labels, title and custom x-axis tick labels, etc.\n",
        "ax.set_ylabel('Average Attack Rating')\n",
        "ax.set_title('Self-Bypass Attack Average Severity Score')\n",
        "ax.set_xticks(x + width, xlabels)\n",
        "ax.legend(loc='upper left', ncols=4)\n",
        "ax.set_ylim(0, 5)\n",
        "plt.savefig(\"self_bypass_average_severity_by_prompt_type.pdf\")\n",
        "plt.show()\n"
